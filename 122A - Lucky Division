#include<bits/stdc++.h>
using namespace std;

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

#define ll long long

//====================================

vector<ll> v = {4,7,44,47,74,77,444,447,474,477,744,747,774,777};

void solve(){
    ll n; cin>>n;
    for(auto i: v){
        if(n%i == 0){
            cout<<"YES" <<'\n';
            return;
        }
    }
    cout<<"NO" <<'\n';
}


int main(){
    optimize();

    ll t=1;
    // cin>>t;
    while(t--)
        solve();
}
